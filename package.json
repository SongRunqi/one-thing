{
  "name": "0neThing",
  "version": "0.1.0",
  "description": "A chat ",
  "main": "./out/main/index.js",
  "type": "module",
  "scripts": {
    "dev": "electron-vite dev",
    "start": "electron-vite preview",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "build": "electron-vite build",
    "build:check": "npm run typecheck && electron-vite build",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "postinstall": "electron-builder install-app-deps",
    "release": "bash scripts/release.sh",
    "lint": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "e2e": "playwright test --config playwright.config.ts",
    "e2e:debug": "playwright test --config playwright.config.ts --debug"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.1",
    "@ai-sdk/deepseek": "^2.0.1",
    "@ai-sdk/devtools": "^0.0.3",
    "@ai-sdk/google": "^3.0.1",
    "@ai-sdk/openai": "^3.0.1",
    "@ai-sdk/openai-compatible": "^2.0.1",
    "@antv/infographic": "^0.2.3",
    "@modelcontextprotocol/sdk": "^1.25.0",
    "@nut-tree-fork/nut-js": "^4.2.6",
    "@openrouter/sdk": "^0.3.10",
    "@pierre/diffs": "^1.0.4",
    "@tanstack/vue-virtual": "^3.13.18",
    "@types/markdown-it": "^14.1.2",
    "@xenova/transformers": "^2.17.2",
    "@zip.js/zip.js": "^2.8.11",
    "ai": "^6.0.3",
    "axios": "^1.4.0",
    "better-sqlite3": "^12.5.0",
    "diff": "^8.0.2",
    "electron-store": "^8.1.0",
    "eventemitter3": "^5.0.1",
    "execa": "^9.6.1",
    "handlebars": "^4.7.8",
    "highlight.js": "^11.11.1",
    "lottie-web": "^5.13.0",
    "lucide-vue-next": "^0.562.0",
    "lunar-javascript": "^1.7.7",
    "markdown-it": "^14.1.0",
    "markdown-it-mathjax3": "^5.2.0",
    "mathjax": "^3.2.2",
    "pinia": "^2.1.3",
    "sqlite-vec": "^0.1.7-alpha.2",
    "tree-sitter-bash": "^0.25.1",
    "uuid": "^13.0.0",
    "vue": "^3.3.4",
    "web-tree-sitter": "^0.26.3",
    "yaml": "^2.8.2",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.58.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/diff": "^7.0.2",
    "@types/handlebars": "^4.0.40",
    "@types/node": "^22.19.1",
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "electron-vite": "^5.0.0",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.2.7",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.1.8"
  }
}
