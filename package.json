{
  "name": "ai-chat-electron",
  "version": "0.1.0",
  "description": "An Electron app for chatting with AI",
  "main": "dist/main/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:electron": "NODE_ENV=development electron main.js",
    "dev:all": "concurrently \"npm run dev\" \"wait-on http://127.0.0.1:5173 && npm run dev:electron\"",
    "build": "vite build && node build-main.js",
    "build:electron": "npm run build && electron-builder",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "electron-store": "^8.1.0",
    "highlight.js": "^11.11.1",
    "marked": "^17.0.1",
    "pinia": "^2.1.3",
    "uuid": "^9.0.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.3",
    "concurrently": "^8.2.0",
    "electron": "^39.2.7",
    "electron-builder": "^24.6.4",
    "electron-is-dev": "^2.0.0",
    "eslint": "^8.43.0",
    "eslint-plugin-vue": "^9.0.0",
    "typescript": "^5.1.6",
    "vite": "^7.2.7",
    "vue-tsc": "^3.1.8",
    "wait-on": "^7.0.1"
  }
}
