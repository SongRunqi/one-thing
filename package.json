{
  "name": "0neThing",
  "version": "0.1.0",
  "description": "An Electron app for chatting with AI",
  "main": "dist/main/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:main": "tsc -p tsconfig.electron.json --watch",
    "dev:preload": "node build-preload.js --watch",
    "dev:electron": "NODE_ENV=development electron dist/main/index.js",
    "dev:all": "concurrently \"vite\" \"tsc -p tsconfig.electron.json --watch\" \"node build-preload.js --watch\" \"wait-on http://127.0.0.1:5173 && NODE_ENV=development electron dist/main/index.js\"",
    "build": "vite build && npm run build:main && npm run build:preload",
    "build:main": "tsc -p tsconfig.electron.json",
    "build:preload": "node build-preload.js",
    "copy:preload-pkg": "node -e \"const fs=require('fs');fs.mkdirSync('dist/preload',{recursive:true});fs.copyFileSync('src/preload/package.json','dist/preload/package.json')\"",
    "build:electron": "npm run build && electron-builder",
    "preview": "vite preview",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.56",
    "@ai-sdk/deepseek": "^1.0.32",
    "@ai-sdk/openai": "^2.0.86",
    "@modelcontextprotocol/sdk": "^1.25.0",
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "@xenova/transformers": "^2.17.2",
    "ai": "^5.0.113",
    "axios": "^1.4.0",
    "better-sqlite3": "^12.5.0",
    "electron-store": "^8.1.0",
    "execa": "^9.6.1",
    "highlight.js": "^11.11.1",
    "marked": "^17.0.1",
    "pinia": "^2.1.3",
    "sqlite-vec": "^0.1.7-alpha.2",
    "uuid": "^9.0.0",
    "vue": "^3.3.4",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/better-sqlite3": "^7.6.13",
    "@vitejs/plugin-vue": "^6.0.3",
    "concurrently": "^8.2.0",
    "electron": "^39.2.7",
    "electron-builder": "^24.6.4",
    "electron-is-dev": "^2.0.0",
    "electron-rebuild": "^3.2.9",
    "esbuild": "^0.27.1",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "typescript": "^5.1.6",
    "vite": "^7.2.7",
    "vue-tsc": "^3.1.8",
    "wait-on": "^7.0.1"
  }
}
